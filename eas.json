{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.19.4",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "test": {
      "distribution": "internal",
      "node": "20.19.4",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      }
    },
    "test-simulator": {
      "distribution": "internal",
      "node": "20.19.4",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      }
    },
    "preview": {
      "autoIncrement": true,
      "node": "20.19.4",
      "ios": {
        "distribution": "store",
        "image": "macos-sequoia-15.5-xcode-26.0",
        "resourceClass": "m-medium"
      },
      "android": {
        "distribution": "internal",
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "node": "20.19.4",
      "ios": {
        "image": "macos-sequoia-15.5-xcode-26.0",
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "karamvir.mangat@uvconsulting.net",
        "ascAppId": "6738724528",
        "appleTeamId": "743ZL7U46T"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json"
      }
    },
    "production": {
      "ios": {
        "appleId": "karamvir.mangat@uvconsulting.net",
        "ascAppId": "6738724528",
        "appleTeamId": "743ZL7U46T"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json"
      }
    }
  }
}
