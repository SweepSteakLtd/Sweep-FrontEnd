{
  "name": "sweepsteak-frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:test:android": "eas build --profile test --platform android",
    "build:test:ios": "eas build --profile test --platform ios",
    "build:test:all": "eas build --profile test --platform all",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:all": "eas build --profile production --platform all",
    "build:web": "expo export -p web -c --output-dir dist",
    "test:android": "eas build:run --profile test --platform android",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "export-mocks": "node scripts/exportMocks.js",
    "generate-schemas": "tsx scripts/generateSchemas.ts",
    "verify-schemas": "tsx scripts/verifySchemas.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "@tanstack/react-query": "^5.62.7",
    "expo": "~52.0.0",
    "expo-clipboard": "~7.0.1",
    "expo-font": "~13.0.4",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.0",
    "firebase": "^12.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-date-picker": "^5.0.13",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-international-phone-number": "^0.11.2",
    "react-native-keyboard-controller": "^1.19.5",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.19.13",
    "styled-components": "^6.1.13",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/react": "~18.3.12",
    "@types/styled-components": "^5.1.34",
    "@types/styled-components-react-native": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
